calc_ANPPt_est = function(GI, ANPPt_max, LAI_expcoef = 0.015) {
Pl = 0.6 + (0.24*GI)
LAI = (Pl/0.6) - ( LAI_expcoef * exp(4.6*GI) )
ANPPt_est = LAI * ANPPt_max
return(ANPPt_est)
}
