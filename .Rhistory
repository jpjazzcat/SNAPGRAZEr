calc_Lg = function(D, d, n, W = NA, Cg = NA) {
if(is.numeric(Cg)) {
Cg = Cg
} else if(is.numeric(W)) {
Cg = 2*(5300+770*ln(W))
} else {print("You need to either provide Cg (daily consumption) or W (animal body size)")}
Lg = D*d*Cg*n*10^(-4)
return(Lg)
}
