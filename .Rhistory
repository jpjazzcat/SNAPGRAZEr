calc_PDSOCt = function(ANPPt_est, BNPPt_est, LIGCELL, GI, Fire) {
PDSOCt = 0.45 * ( (LIGCELL*ANPPt_est*(1-GI)*(1-Fire)) + (LIGCELL+0.05)*BNPPt_est )
return(PDSOCt)
}
