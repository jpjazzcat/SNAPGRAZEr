calc_SOCeq = function(PDSOCt, DDSOCt, SAND, RAIN) {
WETDAYS = 240 * ( (0.00044*RAIN) - 0.025)
RESPcorrection = 0.7 + (0.3 * (SAND/100))
SOCeq = (PDSOCt + DDSOCt + (WETDAYS*0.579*RESPcorrection) ) / ( 0.00044*WETDAYS*RESPcorrection )
return(SOCeq)
}
